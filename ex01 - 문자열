#include<stdio.h>
#include<time.h>
#include<stdlib.h>
#include<string.h>

void input(char a[][100]){
	for (int i = 0; i < 5; i++){
		printf("Input%d : ", i + 1);
		gets(a[i]);
	}
}

void selection(char a[][100]){
	int i, j;
	char t[100];
	for (i = 0; i < 4; i++){
		for (j = i + 1; i < 5; i++){
			if (strcmp(a[i],a[j])>0){ //strcmp : 앞에것이 크면 1, 같으면 0, 작으면 -1을 리턴함.
				strcpy(t, a[i]);
				strcpy(a[i], a[j]);
				strcpy(a[j], t);
			}
		}
	}
}
void output(char a[][100]){
	for (int i = 0; i<5 ; i++){
		printf(" %s", a[i]);
	}
}




int main(){
	char a[5][100];
	input(a);
	printf("정렬전 : "); output(a);
	selection(a);
	printf("\n정렬후 : "); output(a);
	return 0;
}
